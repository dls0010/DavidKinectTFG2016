<Window x:Class="DavidKinectTFG2016.recursosAdministrador.EditarTerapeutas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DavidKinectTFG2016.recursosAdministrador"
        mc:Ignorable="d"
        Title="Editar Terapeutas" Height="400" Width="500" Loaded="Window_Loaded" WindowState="Maximized" Closed="Window_Closed" Icon="/DavidKinectTFG2016;component/images/icon.png">
    <Grid>
        <Viewbox Stretch="Uniform" Margin="10,0,10,10">
            <Grid>
                <Button x:Name="buttonModificar" Content="{Binding buttonModificar, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="76" Click="buttonModificar_Click" Background="#FF73FF23"/>
                <Button x:Name="buttonCancelar" Content="{Binding buttonCancelar, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Center" VerticalAlignment="Top" Width="75" Margin="75,0,-75,0" Click="buttonCancelar_Click" Background="#FFFF2C2C"/>
                <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top"/>
            </Grid>
        </Viewbox>
    </Grid>
</Window>
