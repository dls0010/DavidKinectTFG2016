<Window x:Class="DavidKinectTFG2016.recursosAdministrador.EditarTerapeutas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DavidKinectTFG2016.recursosAdministrador"
        mc:Ignorable="d"
        Title="Editar Terapeutas" Height="400" Width="500" Loaded="Window_Loaded" WindowState="Maximized" Closed="Window_Closed" Icon="/DavidKinectTFG2016;component/images/icon.png">
    <Grid>
        <Button x:Name="buttonModificar" Content="{Binding buttonModificar, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Center" VerticalAlignment="Top" Width="75" Click="buttonModificar_Click" Background="#FF65FF0D" Margin="159,0,258,0"/>
        <Button x:Name="buttonCancelar" Content="{Binding buttonCancelar, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Center" VerticalAlignment="Top" Width="75" Margin="259,0,158,0" Click="buttonCancelar_Click" Background="#FFFF2C2C"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Center" Margin="10,52,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
