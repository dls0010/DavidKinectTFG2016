        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DavidKinectTFG2016.recursosPaciente"
        mc:Ignorable="d"
        Title="Consulta Historial" Height="300" Width="400" Loaded="Window_Loaded" WindowState="Maximized" Icon="/DavidKinectTFG2016;component/images/icon.png">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelTituloHistorialPaciente" Content="{Binding labelTituloHistorialPaciente, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.388,0.114"/>

    </Grid>
</Window>
