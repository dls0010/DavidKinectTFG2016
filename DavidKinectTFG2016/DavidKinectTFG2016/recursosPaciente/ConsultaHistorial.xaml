<Window x:Class="DavidKinectTFG2016.recursosPaciente.ConsultaHistorial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DavidKinectTFG2016.recursosPaciente"
        mc:Ignorable="d"
        Title="Consulta Historial" Height="300" Width="400" Loaded="Window_Loaded" WindowState="Maximized" Icon="/DavidKinectTFG2016;component/images/icon.png">
    <Grid HorizontalAlignment="Center">
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Center" Margin="10,34,0,0" VerticalAlignment="Top" VerticalScrollBarVisibility="Visible"/>
        <Grid HorizontalAlignment="Center">
            <Label x:Name="labelTituloHistorialPaciente" Content="{Binding labelTituloHistorialPaciente, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.388,0.114"/>
            <Button x:Name="buttonCancelar" Content="{Binding buttonCancelar, Source={StaticResource IdiomasRes}}" HorizontalAlignment="Left" Margin="202,0,0,0" VerticalAlignment="Top" Width="82" Click="buttonCancelar_Click" Background="#FFFF2C2C" Height="28"/>
        </Grid>
    </Grid>
</Window>
